// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Message{
  messageId String @id @default(uuid())
  messageContent String
  prompt String
  messageAudioPath String

}
